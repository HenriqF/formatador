      "16": "icons/16.png",
      "32": "icons/32.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "web_accessible_resources": [
  {
    "resources": ["src/mapas/"],
    "matches": ["<all_urls>"]
  }
  ],
  "permissions": ["storage"],
  "background": {
    "service_worker": "src/service-worker.js"
  }
}